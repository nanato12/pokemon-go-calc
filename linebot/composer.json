{
    "name": "pokemon-go-calc/linebot",
    "description": "Pokemon GO IV Extractor LINE Bot",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "generated/iv-extractor-client"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "linecorp/line-bot-sdk": "^12.0",
        "pokemon-go-calc/iv-extractor-client": "*",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PokemonGoCalc\\LineBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PokemonGoCalc\\LineBot\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
